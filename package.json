{
  "name": "ip-location-api",
  "version": "0.2.1",
  "description": "Fast location lookup from IP address",
  "keywords": [
    "location",
    "lookup",
    "geo",
    "geoip",
    "geolite",
    "maxmind",
    "ip",
    "ipv4",
    "ipv6",
    "ip-location-db",
    "country",
    "city"
  ],
  "scripts": {
    "test": "npx jasmine",
    "postinstall": "node script/updatedb-first.mjs",
    "updatedb": "node script/updatedb.mjs",
    "updatedb-cjs": "node script/updatedb.cjs",
    "updatedb-browser": "node script/updatedb.mjs ILA_BROWSER_TYPE=country ILA_IP_LOCATION_DB=geo-whois-asn",
    "browser": "npx http-server browser -p 8321",
    "cjs": "node script/convert-to-cjs.mjs"
  },
  "author": "sapics",
  "homepage": "https://github.com/sapics/ip-location-api",
  "repository": {
    "type": "git",
    "url": "git://github.com/sapics/ip-location-api.git"
  },
  "license": "Multiple licenses",
  "type": "module",
  "engines": {
    "node": ">=14.8.0"
  },
  "main": "cjs/main.cjs",
  "module": "src/main.mjs",
  "exports": {
    "import": "./src/main.mjs",
    "require": "./cjs/main.cjs",
    "node": "./src/main.mjs",
    "default": "./cjs/main.cjs"
  },
  "files": [
    "src/*.mjs",
    "!src/brower*.mjs",
    "cjs/*.cjs",
    "spec",
    "script/*.mjs",
    "script/*.cjs",
    "LICENSE",
    "EULA",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.7.7",
    "countries-list": "^3.1.1",
    "fast-csv": "^5.0.1",
    "ip-address": "^9.0.5",
    "yauzl": "^3.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "dayjs": "^1.11.13",
    "doc999tor-fast-geoip": "^1.1.253",
    "eslint": "^8.57.0",
    "geoip-country": "^5.0.202408302251",
    "geoip-lite": "^1.4.10",
    "globals": "^15.9.0",
    "http-server": "^14.1.1",
    "jasmine": "^5.2.0",
    "rollup-plugin-ignore": "^1.0.10"
  }
}
